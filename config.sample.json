{
  "server": {
    "url": "https://teamcity.server",
    "user": "user",
    "password": "password",
    "cert": "pathToP12Cert",
    "pollInterval": 10000
  },
  "sources": [{
    "name": "default",
    "priority": 1,
    "reportingColors": {
      "ok": "green",
      "failed": "red",
      "noConnection": "yellow"
    },
    "buildLocators": [{
        "name": "Build",
        "locator": "buildType:BuiltTypeName,count:1,branch:develop"
    }]
  }],
  "blinkStick": {
    "numberOfLeds": 8
  }
}
